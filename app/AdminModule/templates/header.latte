<table class="primaryMenu">
	<tr>
		 {foreach $primaryMenu as $key => $value}
		<td>
			<a n:href="{$value}"
			class="{ifCurrent $value}active{/ifCurrent}"
			><span>{$value}</span></a>
			</td>
		{/foreach}
	</tr>
</table>

<table class="secondaryMenu">
	<tr>
		 {foreach $secondaryMenu as $key => $value}
		<td>
			<a n:href="{$value}"
			class="{ifCurrent $value}active{/ifCurrent}"
			><span>{$value}</span></a>
			</td>
		{/foreach}
	</tr>
</table>


